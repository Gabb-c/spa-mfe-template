{
  "name": "@mfe/template",
  "types": "dist/mfe-template.d.ts",
  "scripts": {
    "analyze": "webpack --mode=production --env analyze",
    "build": "webpack --mode=production",
    "postbuild": "tsc",
    "check-format": "prettier --check .",
    "format": "prettier --write .",
    "lint": "eslint src --ext js,ts,tsx",
    "lint:fix": "eslint src --ext js,ts,tsx",
    "prepare": "is-ci || husky install",
    "start": "webpack serve --port 32004",
    "start:dev": "webpack serve --mode development --port 32004 --env isLocal --progress --client-progress",
    "start:standalone": "webpack serve --env standalone",
    "test:ci": "jest --ci --runInBand --bail",
    "test:cov": "cross-env BABEL_ENV=test jest --coverage",
    "test:dev": "cross-env BABEL_ENV=test jest --verbose --colors --expand --maxWorkers=50% --detectOpenHandles --errorOnDeprecated",
    "test:watch": "cross-env BABEL_ENV=test jest --watch --verbose --colors --expand --maxWorkers=25%"
  },
  "dependencies": {
    "@types/systemjs": "^6.1.1",
    "@types/webpack-env": "^1.16.3",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-router": "^6.2.1",
    "react-router-dom": "^6.2.1",
    "single-spa": "^5.9.3",
    "single-spa-react": "^4.6.1"
  },
  "devDependencies": {
    "@babel/core": "^7.17.0",
    "@babel/plugin-transform-runtime": "^7.17.0",
    "@babel/preset-env": "^7.16.11",
    "@babel/preset-react": "^7.16.7",
    "@babel/preset-typescript": "^7.16.7",
    "@babel/runtime": "^7.17.0",
    "@commitlint/cli": "^16.1.0",
    "@commitlint/config-conventional": "^16.0.0",
    "@testing-library/jest-dom": "^5.16.2",
    "@testing-library/react": "^12.1.2",
    "@types/express": "^4.17.13",
    "@types/jest": "^27.4.0",
    "@types/node": "^17.0.15",
    "@types/react": "^17.0.39",
    "@types/react-dom": "^17.0.11",
    "@types/testing-library__jest-dom": "^5.14.2",
    "@typescript-eslint/eslint-plugin": "^5.10.2",
    "@typescript-eslint/parser": "^5.10.2",
    "babel-jest": "^27.5.0",
    "cross-env": "^7.0.3",
    "eslint": "^8.8.0",
    "eslint-config-airbnb-typescript": "^16.1.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-eslint-comments": "^3.2.0",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-jest": "^26.1.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-promise": "^6.0.0",
    "eslint-plugin-react": "^7.28.0",
    "eslint-plugin-unicorn": "^40.1.0",
    "husky": "^7.0.4",
    "identity-obj-proxy": "^3.0.0",
    "is-ci": "^3.0.1",
    "jest": "^27.5.0",
    "jest-transform-stub": "^2.0.0",
    "lint-staged": "^12.3.4",
    "prettier": "^2.5.1",
    "pretty-quick": "^3.1.3",
    "sort-package-json": "^1.54.0",
    "ts-config-single-spa": "^3.0.0",
    "ts-jest": "^27.1.3",
    "typescript": "^4.5.5",
    "webpack": "^5.68.0",
    "webpack-cli": "^4.9.2",
    "webpack-config-single-spa-react": "^4.0.3",
    "webpack-config-single-spa-react-ts": "^4.0.3",
    "webpack-config-single-spa-ts": "^4.1.2",
    "webpack-dev-server": "^4.7.4",
    "webpack-merge": "^5.8.0"
  }
}
